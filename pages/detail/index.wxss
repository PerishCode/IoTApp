.container {
  height: 100%;
  width: 100%;
}

.container > .title {
  text-align: center;
  position: absolute;
  bottom: 15px;
  width: 100%;
  font-size: 18px;
  line-height: 18px;
  font-weight: 700;
}

.list .item {
  font-size: 14px;
  position: relative;
  padding: 5px 0px;
}

.list .item .index {
  display: inline;
  font-weight: 700;
}

.list .item .value {
  float: right;
}
